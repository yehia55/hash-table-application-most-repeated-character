package hashing;
import java.util.*; 
public class ChainingLinkedList {
    static Scanner in=new Scanner(System.in);
    static int maxsize;
    static int size;
  public static Node[]table;
    public ChainingLinkedList(int tablesize){
     table=new Node[tablesize];
    size =0;
    maxsize =tablesize;
    }
     public static int hashcode (char x ){
        Character c = x; 
        int i = c.hashCode(); 
        return i;
    }
     public static int MyHash(char x){
     int hashval=hashcode(x);
     hashval%=table.length;
     if (hashval<0){
     hashval+=table.length;
     }
     return hashval;
     }
     public static void add(char x){
     size ++;
     int pos =MyHash(x);
     Node pointer=new Node (x);
     if ( pointer==null){
     table [pos]=pointer;
     }
     else 
        pointer.next= table [pos];
        table [pos]=pointer;
     }
     public static void print (){
     for(int i=0;i<table.length;i++){
      if (table[i]==null)
          continue;
     System.out.print("cell"+(i+1)+": ");
     Node n=table[i];
     while (n!=null){
     System.out.print(n.data+" ");
     n=n.next;
     }
       System.out.println();
     }
}
