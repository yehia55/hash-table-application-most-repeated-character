     for(int i=0;i<52;i++){
         if (arr[i]==0)
             continue;
   System.out.println(arr2[i]+" "+arr[i]);
   }
    int max=0;
    int index=-1;
          for(int i=0;i<52;i++){
              if (arr[i]>max){
                 max=arr[i]; 
